{
  "problemMatcher": [
    {
      "owner": "checkstyle",
      "pattern": [
        {
          "regexp": "^\\s*([\\S].+)$",
          "file": 1
        },
        {
          "regexp": "^\\s*(\\d+)(?::(\\d+)) - (\\S+) - (.+)$",
          "line": 1,
          "column": 2,
          "severity": 3,
          "message": 4,
          "loop": true
        }
      ]
    }
  ]
}
