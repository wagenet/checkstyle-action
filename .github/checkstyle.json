{
  "problemMatcher": [
    {
      "owner": "checkstyle",
      "pattern": [
        {
          "regexp": "^\\[(WARNING|ERROR)\\] ([^:].+):\\[(\\d+)(?::(\\d+))?\\] \\((.+)\\) (.+)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
